<%- layout('layout/photolayout') %>
<main class="main-body">
    <div class="body-content-photography">
        <div class="photography-title">
            <h1>Photography</h1>
            <hr>
        </div>
        <div class="collections">
            <h2>Collections</h2>
        </div>
        <div class="photo-grid">
            <div class="photo-container landscape">
                <a href="/photography/travel-landscape" ><img class="photo-thumbnail wide" src="/images/collections/Landscape/Landscape_horizontal_6.webp" alt="Landscape"></a>
